# schema.graphqls
type User {
  id: ID!
  username: String!
  password: String!
}

type Query {
  users: [User!]!
  user(id: ID!): User
}

type Mutation {
  createUser(username: String!, password: String!): User!
  updateUser(id: ID!, username: String, password: String): User!
  deleteUser(id: ID!): User!
}
